{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "setup": "git config core.hooksPath git-hooks",
    "build": "webpack --mode production",
    "check": "run-p --aggregate-output --continue-on-error check:*",
    "check:types": "tsc --project tsconfig.json",
    "check:eslint": "eslint --ignore-path .gitignore --cache --cache-strategy content --max-warning=0 .",
    "check:prettier": "prettier --check --ignore-path .gitignore .",
    "fix": "run-s --continue-on-error --print-name fix:*",
    "fix:eslint": "npm run check:eslint -- --fix",
    "fix:prettier": "npm run check:prettier -- --write"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "html-webpack-plugin": "^5.5.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "tslib": "^2.5.3"
  }
}
