{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "setup": "git config core.hooksPath git-hooks",
    "start": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "check": "run-p --aggregate-output --continue-on-error check:*",
    "check:types": "tsc --project tsconfig.json",
    "check:eslint": "eslint --ignore-path .gitignore --cache --cache-strategy content --max-warnings=0 .",
    "check:prettier": "prettier --check --ignore-path .gitignore .",
    "fix": "run-s --continue-on-error --print-name fix:*",
    "fix:eslint": "npm run check:eslint -- --fix",
    "fix:prettier": "npm run check:prettier -- --write"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "babel-loader": "^9.1.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "html-webpack-plugin": "^5.5.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "react-refresh": "^0.14.0",
    "ts-loader": "^9.4.3",
    "type-fest": "^3.12.0",
    "typescript": "^5.1.3",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "dependencies": {
    "@total-typescript/ts-reset": "^0.4.2",
    "mqtt": "^4.3.7",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.5.3"
  }
}
